int[][] board;
int scale = 35;

void setup() {
  size(600, 700);
  frameRate(4);
  board = new int[10][20];
}

void draw() {
  background(200);

  for ( int i = 0; i < 10; i++ ) {    //10 columns
    for ( int j = 0; j < 20; j++ ) {  //20 rows
      rect( i*scale, j*scale, scale, scale );
    }
  }
}